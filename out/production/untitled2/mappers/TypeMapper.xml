<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sy.mapper.TypeMapper">
    <select id="getTypeByBrandId" parameterType="int" resultType="Type">
        select id,`name`,brandid from t_type where brandid=#{brandid}
    </select>
</mapper>